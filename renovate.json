{
  "enabledManagers": ["dockerfile", "tekton"],
  "dockerfile": {
    "enabled": true,
    "ignoreTests": true,
    "automergeType": "pr",
    "automerge": true
  },
  "tekton": {
    "enabled": true,
    "fileMatch": [
      "^\\.tekton/.*\\.yaml$",
      "\\.yaml$",
      "\\.yml$"
    ],
    "ignoreTests": true,
    "automergeType": "pr",
    "automerge": true
  }
}
